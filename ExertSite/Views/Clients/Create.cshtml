@model ExertSite.Models.Clients

@{
    ViewData["Title"] = "Create";
    Layout = "/Views/Shared/_Admin.cshtml";
    ViewData["pageTitle"] = "Clients";

}


<div class="row">
    <div class="container-fluid card">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ClientImage" class="control-label"></label>
                <input type="file" asp-for="ClientImage" class="form-control" />
                <span asp-validation-for="ClientImage" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
        
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
